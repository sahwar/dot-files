# http://pyropus.ca/software/getmail/configuration.html

[retriever]
type = SimpleIMAPSSLRetriever
server = imap.gmail.com
mailboxes = ("[Gmail]/All Mail",)
username = tap@cin.ufpe.br

[destination]
type = Maildir
path = /media/mail/tap@cin.ufpe.br/

[options]
verbose = 2
message_log = ~/.tap@cin.ufpe.br-getmail.log

# retrieve only new messages
# if set to true it will re-download ALL messages every time!
read_all = false

# do not alter messages
delivered_to = false
received = false
