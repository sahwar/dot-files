URxvt.perl-lib: .config/urxvt
URxvt.perl-ext-common: fullscreen
URxvt.keysym.F11: perl:fullscreen:switch
URxvt.background: #1C1C1C
URxvt.foreground: #FFFFFF
URxvt.font: xft:Ubuntu Mono:pixelsize=22
/* URxvt.boldFont: xft:Ubuntu Mono:bold:pixelsize=22 */
/* URxvt.italicFont: xft:Ubuntu Mono:italic:pixelsize=22:autohint=true */
/* URxvt.boldItalicFont: xft:Ubuntu Mono:bold:italic:pixelsize=22:autohint=true */
URxvt.letterSpace: -1
URxvt.url-launcher: /usr/bin/firefox
URxvt.underlineURLs: true
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight
URxvt.scrollBar: false
URxvt*saveLines: 0
! gnome-terminal Tango theme
! black
URxvt.color0 : #2E2E34343636
URxvt.color8 : #555557575353
! red
URxvt.color1 : #CCCC00000000
